pipeline {
    agent {
        docker {
            image 'openjdk:12'
            args '-p 1000:65535'
        }
    }
    stages {
        stage('Build') {
            steps {
                sh 'cd src/com/hangman/ && javac -encoding UTF-8 *.java'
		sh 'cd src/com/hangman/ && java Server'
		sh 'echo this is server sssss'
            }
        }
    }
}
